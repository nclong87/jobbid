<?php 
foreach($this->roots as $root) {
	if(isset($root->childs)) {
	?>
	<li>
		<a href="#"><?php echo $root->menu_name?></a>
		<ul>
			<?php 
			foreach($root->childs as $menu)
				echo '<li><a href="'.$this->baseUrl().$menu->url.'">'.$menu->menu_name.'</a></li>';
			?>
		</ul>
	</li>
	<?php
	}
}
?>